{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Client Mod Template",
  "description": "This is an client only mod template!",
  "authors": [
    "Me"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/Vulp35/fabric-mod-template",
    "issues": "https://github.com/Vulp35/fabric-mod-template/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.fabricmc.example.ExampleMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
  }
}
